/*Example 4: Make the algorithm that writes the name of the programmer on the screen 10 times */
import java.util.Scanner;
public class CodeCraftswououman {
    public static void main(String[]args){
        Scanner input= new Scanner(System.in);
        String name;
        System.out.println("Hello, what's your name? Please enter your name.");
        name=input.nextLine();
        for(int i=0; i<10; i++){
            System.out.println(name);
        }
    }
    
}
